    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="selectProgramLabel" prefHeight="48.0" prefWidth="491.0" text="Please select a program from below:" textAlignment="CENTER" textFill="#21865f">
